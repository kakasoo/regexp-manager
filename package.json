{
    "name": "regexp-manager",
    "version": "0.12.0",
    "description": "regexp builder for node.js developer",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "files": [
        "./dist"
    ],
    "scripts": {
        "prebuild": "rimraf dist && npm install",
        "build": "tsc",
        "test": "node --watch --require ts-node/register --test src/test/new-feature/*.spec.ts",
        "test:cov": "jest --coverage"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/kakasoo/regexp-manager"
    },
    "keywords": [
        "regexp",
        "bilder",
        "regular",
        "expression"
    ],
    "author": "kakasoo <kscodebase@gmail.com>",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/kakasoo/regexp-manager/issues"
    },
    "homepage": "https://github.com/kakasoo/regexp-manager#readme",
    "dependencies": {
        "typescript": "^4.9.4",
        "typia": "^3.8.4"
    },
    "devDependencies": {
        "@types/jest": "^29.2.6",
        "@types/node": "^20.1",
        "rimraf": "^4.0.5",
        "ts-node": "^10.9.1"
    },
    "jest": {
        "moduleFileExtensions": [
            "js",
            "json",
            "ts"
        ],
        "rootDir": "./src",
        "transformIgnorePatterns": [
            "^.+\\.js$"
        ],
        "testRegex": ".*\\.spec\\.ts$",
        "transform": {
            "^.+\\.(t|j)s$": "ts-jest"
        },
        "coverageDirectory": "../coverage",
        "testEnvironment": "node"
    }
}
